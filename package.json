{
  "name": "astro-sass-starter-pack",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build && pagefind --site dist && cp -r dist/pagefind public/",
    "preview": "astro preview",
    "astro": "astro",
    "prettier": "prettier --write 'src/**/*.{ts,js,astro,scss}'",
    "stylelint": "stylelint --fix 'src/**/*.{astro,scss}'",
    "eslint": "eslint --fix 'src/**/*.{ts,js,astro}'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@astrojs/check": "^0.5.6",
    "astro": "^4.4.0",
    "gsap": "^3.12.5",
    "newt-client-js": "^3.3.0",
    "pagefind": "^1.0.4",
    "sass": "^1.68.0",
    "sharp": "^0.33.2"
  },
  "devDependencies": {
    "@iconify-json/mdi": "^1.1.64",
    "@typescript-eslint/parser": "^6.7.3",
    "astro-icon": "^1.1.0",
    "eslint": "^8.50.0",
    "eslint-plugin-astro": "^0.29.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "modern-css-reset": "^1.4.0",
    "postcss-html": "^1.5.0",
    "prettier": "^3.0.3",
    "prettier-plugin-astro": "^0.12.0",
    "stylelint": "^16.2.1",
    "stylelint-config-recess-order": "^5.0.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-prettier": "^5.0.0"
  }
